{
    "name": "pecyn/api-wrapper",
    "description": "Api wrapper",
    "keywords": ["api", "wrapper", "guzzle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Davies",
            "email": "lee@lmcdavies.co.uk"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload": {
